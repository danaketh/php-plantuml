{
  "name": "danaketh/php-plantuml",
  "description": "Generate PlantUML diagrams from your PHP code",
  "type": "library",
  "keywords": [
    "php",
    "plantuml",
    "diagram",
    "generate",
    "generator",
    "uml"
  ],
  "homepage": "https://github.com/danaketh/php-plantuml",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Tlach",
      "email": "daniel@tlach.cz"
    }
  ],
  "require": {
    "php": ">= 8.0",
    "nikic/php-parser": "^4.14",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.8",
    "squizlabs/php_codesniffer": "^3.7",
    "slevomat/coding-standard": "^8.3",
    "rector/rector": "^0.13.10"
  },
  "autoload": {
    "psr-4": {
      "PhpPlantUML\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpPlantUML\\": "tests/"
    }
  },
  "bin": [
    "bin/php-plantuml"
  ],
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon",
    "codesniffer": "vendor/bin/phpcs -s",
    "codesniffer-fix": "vendor/bin/phpcbf"
  }
}
